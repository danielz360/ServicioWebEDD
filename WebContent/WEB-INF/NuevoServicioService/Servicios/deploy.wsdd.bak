<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from NuevoServicioService WSDL service -->

  <service name="NuevoServicio" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://Servicios"/>
      <parameter name="wsdlServiceElement" value="NuevoServicioService"/>
      <parameter name="schemaQualified" value="http://Servicios"/>
      <parameter name="wsdlServicePort" value="NuevoServicio"/>
      <parameter name="className" value="Servicios.NuevoServicioSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="NuevoServicio"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="decirHola" qname="operNS:DecirHola" xmlns:operNS="http://Servicios" returnQName="retNS:DecirHolaReturn" xmlns:retNS="http://Servicios" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:Nombre" xmlns:pns="http://Servicios" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="decirHola"/>

  </service>
</deployment>
